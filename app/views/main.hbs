<!DOCTYPE HTML>
<html>
    <head>
        <title>{{title}}</title>
    </head>
    <body>
        {{#each feeds}}
        <section>
            <h1>{{title}}</h1>
            {{#each posts}}
                <article>
                    <h2>{{title}}</h2>
                    <span>{{description}}</span>
                    <p>{{summary}}</p>
                    {{#if image}}
                        <img src="{{image.url}}">
                    {{/if}}
                </article>
            {{/each}}
        </section>
        {{/each}}
    </body>
</html>